generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model User {
  id String @id @default(auto()) @map("_id") @db.ObjectId
  username String @unique
  password String
  calls Call[]
}

model Call {
  id String @id @default(auto()) @map("_id") @db.ObjectId
  name String
  character String
  scamNumber String @map("scam_number")
  scamCompany String @map("scam_company")
  userId String @db.ObjectId
  user   User   @relation(fields: [userId], references: [id])
}